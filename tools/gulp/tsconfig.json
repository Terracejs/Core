{
	"compilerOptions": {
		"target": "es6",
		
		"lib": [
			"es6", "es2015"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"declaration": true
	}
}